ader.download_app ? \n                <>\n                    <h1>DOWNLOAD APP MUNCUL</h1>\n                </>\n                :\n                <>\n\n                </>\n            }\n            {\n                callbackFromHeader.order_list ?\n                <>\n                    <h1>ORDER LIST KEBUKA</h1>\n                </>\n                :\n                <>\n\n                </>\n            }\n                <div className=\"box-highlight2\">\n                    <LazyLoad>\n                            <Footer/>\n                    </LazyLoad>\n                </div>\n\n        </div>\n        </>\n    )\n}","/Users/new/Desktop/Project/website_react_js/soldays-web/src/Pages/Product/ProductDetail.jsx",["166","167","168","169","170","171","172","173","174","175","176","177","178","179","180","181","182","183","184","185"],"/Users/new/Desktop/Project/website_react_js/soldays-web/src/redux/Reducers/ProductReducers.js",[],"/Users/new/Desktop/Project/website_react_js/soldays-web/src/redux/Reducers/AuthReducers.js",[],"/Users/new/Desktop/Project/website_react_js/soldays-web/src/Pages/Header/Header.jsx",["186","187","188","189","190","191","192","193","194","195","196","197","198","199","200","201","202","203","204","205","206","207","208","209","210","211","212","213","214"],"/Users/new/Desktop/Project/website_react_js/soldays-web/src/Component/Highlight/highlight.jsx",[],"/Users/new/Desktop/Project/website_react_js/soldays-web/src/Component/Effect/img_effect.jsx",[],"/Users/new/Desktop/Project/website_react_js/soldays-web/src/Component/card/Card_Ads.jsx",["215"],"import React,{useState} from 'react'\nimport './Card_Ads.css'\nimport { LazyLoadImage } from 'react-lazy-load-image-component';\nimport ImgEffect from '../Effect/img_effect'\nexport default function Box_Ads_small(data){\n    return (\n        <>\n            <div className=\"box-ads-small-container\">\n                <div className=\"box-ads-inner\">\n                    <div className=\